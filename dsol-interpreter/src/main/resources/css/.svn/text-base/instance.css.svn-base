#load_actions{
	margin:0;
	padding-left:10px;
	float: left;
}

#new_action_link {
	margin-left: 5px;
}

.remove {
	float: right;
}

.actions {
	padding: 10px;
	border: 1px solid #CCCCCC;
	width: auto;
	min-width: 300px;
	float: left;
	margin: 5px;
	border-radius: 5px;
}

.seam_actions {
	border: 1px dashed #CCCCCC;
	padding-bottom: 25px;
}

.removed {
	background-color: grey;
	color: black;
	opacity: 0.4;
	z-index: 1;
}

.put_back {
	display: none;
	opacity: 1;
	color: white;
	position: absolute;
	z-index: 2;
	padding-top: 10px;
	padding-left: 5px;
}

.put_back a:link,.put_back a:visited {
	color: #FF0000;
	text-decoration: none;
	font-weight: bold;
} /* unvisited link */
.concrete_link:link,.concrete_link:visited {
	color: #1c94c4;
	font-weight: bold;
} /* unvisited link */
#form_action_div {
	margin: 10px;
	padding: 5px 4px 0 5px;
	width: 300px;
}

#form_action_div input[type='text'] {
	width: 100%;
}

.ixs {
	padding: 0px;
}

.method {
	padding: 0 0 5px 0;
}

.concrete_input {
	width: 300px;
}

.removed_class{
	text-decoration: line-through; 
}

#planResult{
	margin-top: 5px;
	padding-bottom: 10px;
	border-top: 1px solid #CCCCCC;
	border-collapse: collapse;
}


#planResult h1{
	font-size: 15px;
}